{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include":["./scripts", "readme.md"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "destination":"./jsdoc/",
        "template": "node_modules/docdash"
    },
    "tutorial": {
        "title": "Tutorial One",
        "children": [
            "readme"   
        ]
    }
}