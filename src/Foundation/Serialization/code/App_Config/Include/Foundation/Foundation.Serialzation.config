<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"                                
               xmlns:unicorn="http://www.sitecore.net/xmlconfig/unicorn/">
    <sitecore unicorn:require="On">
        <unicorn>
            <configurations>
                <configuration name="Foundation.Serialization" description="Includes the root tenant folder." extends="Helix.Foundation">
                    <predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
                        <include name="Project.Templates" database="master" path="/sitecore/templates/Project">
                            <exclude children="true" />
                        </include>
                        <include name="Foundation.Templates" database="master" path="/sitecore/templates/Foundation/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.Templates" database="master" path="/sitecore/templates/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Project.Templates.Branches" database="master" path="/sitecore/templates/Branches/Project/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Foundation.Templates.Branches" database="master" path="/sitecore/templates/Branches/Foundation/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.Templates.Branches" database="master" path="/sitecore/templates/Branches/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Project.Layout" database="master" path="/sitecore/layout/Layouts/Project/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Foundation.Layout" database="master" path="/sitecore/layout/Layouts/Foundation/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.Layout.PlaceholderSettings" database="master" path="/sitecore/layout/Placeholder Settings/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.Layout" database="master" path="/sitecore/layout/Layouts/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Project.Renderings" database="master" path="/sitecore/layout/Renderings/Project/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Foundation.Renderings" database="master" path="/sitecore/layout/Renderings/Foundation/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.Renderings" database="master" path="/sitecore/layout/Renderings/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Project.Media" database="master" path="/sitecore/media library/Project/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Foundation.Media" database="master" path="/sitecore/media library/Foundation/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.Media" database="master" path="/sitecore/media library/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Project.System.Settings" database="master" path="/sitecore/system/Settings/Project/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Foundation.System.Settings" database="master" path="/sitecore/system/Settings/Foundation/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Feature.System.Settings" database="master" path="/sitecore/system/Settings/Feature/Habitat Sites">
                            <exclude children="true" />
                        </include>
                        <include name="Core.Templates" database="core" path="/sitecore/templates/Feature/Habitat Sites">       
                            <exclude children="true" />
                        </include>                               
                    </predicate>

                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>